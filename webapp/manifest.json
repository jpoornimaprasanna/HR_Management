{
	"_version": "1.8.0",
	"sap.app": {
		"id": "HrAndEmp.HR_Management",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "HrAndEmp.HR_Management.view.login",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "HrAndEmp.HR_Management.i18n.i18n"
				}
			},
			"empDetails": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "EmployeeDetail.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "HrAndEmp.HR_Management.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "Routelogin",
					"pattern": "Routelogin",
					"target": [
						"Targetlogin"
					]
				},
				{
					"pattern": "EmployeeData/{obj}",
					"name": "EmployeeData",
					"target": "EmployeeData"
				},
				{
					"pattern": "",
					"name": "",
					"target": "Targetlogin"
				},
				{
					"pattern": "AdminPage",
					"name": "AdminPage",
					"target": "AdminPage"
				},
				{
					"pattern": "",
					"name": "login",
					"target": "Targetlogin"
				},
				{
					"pattern": "AdminAddEmployee",
					"name": "AdminAddEmployee",
					"target": "AdminAddEmployee"
				},
				{
					"pattern": "Notification",
					"name": "Notification",
					"target": "Notification"
				},
				{
					"pattern": "EmployeePage/{obj}",
					"name": "EmployeePage",
					"target": "EmployeePage"
				},
				{
					"pattern": "FirstEmpPage",
					"name": "FirstEmpPage",
					"target": "FirstEmpPage"
				}
			],
			"targets": {
				"Targetlogin": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "login",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "HrAndEmp.HR_Management.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"AdminPage": {
					"viewType": "XML",
					"viewName": "AdminPage",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "HrAndEmp.HR_Management.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"EmployeePage": {
					"viewType": "XML",
					"viewName": "EmployeePage"
				},
				"EmployeeData": {
					"viewType": "XML",
					"viewName": "EmployeeData",
					"viewPath": "HrAndEmp.HR_Management.view",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"FirstEmpPage": {
					"viewType": "XML",
					"viewName": "FirstEmpPage",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "HrAndEmp.HR_Management.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"AdminAddEmployee": {
					"viewType": "XML",
					"viewName": "AdminAddEmployee",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "HrAndEmp.HR_Management.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"Notification": {
					"viewType": "XML",
					"viewName": "Notification",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "HrAndEmp.HR_Management.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				}
			}
		}
	}
}